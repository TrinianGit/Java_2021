
public class Test {
	
	
	public static void main(String[] args) {
		
		BusLine bus = new BusLine();
		Position start = new Position2D (10, 10);
		Position end = new Position2D (0, 0);
		bus.addBusLine("114", start, end);
		LineSegment line = new LineSegment(new Position2D(0,5), new Position2D (0,0));
		bus.addLineSegment("114", line);
		line = new LineSegment(new Position2D(10,10), new Position2D (5,10));
		bus.addLineSegment("114", line);
		line = new LineSegment(new Position2D(5,10), new Position2D (0,5));
		bus.addLineSegment("114", line);
		
		System.out.println(bus.getLines());
		
	}
	
	
}
